{
  "manifest_version": 3,
  "name": "Sinais de apostas Betano",
  "version": "1.0",
  "description": "Detecta cupom e envia imagens.",
  "permissions": ["tabs"],
  "host_permissions": ["https://www.betano.bet.br/*"],
  "content_scripts": [
    {
      "matches": ["https://www.betano.bet.br/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
